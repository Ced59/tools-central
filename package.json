    "@angular/platform-server": "^21.0.4",
    "@angular/router": "^21.0.4",
    "@angular/ssr": "^21.0.3",
    "@primeng/themes": "^21.0.1",
    "express": "^4.18.2",
    "ngx-matomo-client": "^9.0.1",
    "primeicons": "^7.0.0",
